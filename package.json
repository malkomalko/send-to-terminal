{
    "name": "send-to-terminal",
    "displayName": "Send to Terminal",
    "description": "Send commands to terminal based on file patterns",
    "version": "0.0.1",
    "publisher": "malkomalko",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:sendToTerminal.run"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "sendToTerminal.run",
                "title": "Send to Terminal - Run"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.0",
        "@types/mocha": "^2.2.42"
    }
}
